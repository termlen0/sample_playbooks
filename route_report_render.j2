{% if sorted_routes.get('static') %}
  STATIC:
  ------
  {% for route in sorted_routes.static %}
    Network: {{ route.NETWORK }}
    Nexthop IP: {{ route.NEXTHOP_IP }}
    Nexthop Interface: {{ route.NEXTHOP_IF }}
    Metric: {{ route.METRIC }}
  {% endfor %}
{% endif %}
{% if sorted_routes.get('bgp') %}
  BGP:
  ----
  {% for route in sorted_routes.bgp %}
    
    Network: {{ route.NETWORK }}
    Nexthop IP: {{ route.NEXTHOP_IP }}
    Nexthop Interface: {{ route.NEXTHOP_IF }}
    Metric: {{ route.METRIC }}
  {% endfor %}
{% endif %}

  
